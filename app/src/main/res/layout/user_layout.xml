<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="210dp">


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewUserName"
        style="@style/TextAppearance.AppCompat.Large"
        android:layout_width="313dp"
        android:layout_height="26dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.143"
        tools:text="Name"
        android:layout_marginLeft="28dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewUserid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUserName"
        app:layout_constraintTop_toBottomOf="@+id/textViewUserName"
        tools:text="Id" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewUserEmail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUserid"
        app:layout_constraintTop_toBottomOf="@+id/textViewUserid"
        tools:text="Email" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewUsergender"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUserEmail"
        app:layout_constraintTop_toBottomOf="@+id/textViewUserEmail"
        tools:text="Gender" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewUsercreatedat"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUsergender"
        app:layout_constraintTop_toBottomOf="@+id/textViewUsergender"
        tools:text="CreatedAt" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/delete"
        android:layout_width="109dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:text="@string/User_delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUsercreatedat"
        app:layout_constraintTop_toBottomOf="@+id/textViewUsercreatedat"
        app:layout_constraintVertical_bias="0.027"
        tools:text="User Delete"
        android:layout_marginLeft="8dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/comments"
        android:layout_width="109dp"
        android:layout_height="36dp"
        android:text="@string/show_com"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/delete"
        app:layout_constraintTop_toBottomOf="@+id/delete"
        app:layout_constraintVertical_bias="0.068"
        tools:text="Comments" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/posts"
        android:layout_width="109dp"
        android:layout_height="36dp"
        android:layout_marginStart="168dp"
        android:text="@string/show_post"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/delete"
        app:layout_constraintTop_toBottomOf="@+id/delete"
        app:layout_constraintVertical_bias="0.09"
        tools:text="Posts"
        android:layout_marginLeft="168dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/TODOS"
        android:layout_width="109dp"
        android:layout_height="36dp"
        android:layout_marginStart="176dp"
        android:text="@string/show_todos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textViewUsercreatedat"
        app:layout_constraintTop_toBottomOf="@+id/textViewUsercreatedat"
        app:layout_constraintVertical_bias="0.0"
        tools:text="TODOS"
        android:layout_marginLeft="176dp" />
</androidx.constraintlayout.widget.ConstraintLayout>